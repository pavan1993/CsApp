{
  "name": "customer-success-backend",
  "version": "1.0.0",
  "description": "Customer Success Analytics Backend API",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon src/server.ts",
    "build": "npm run db:generate && tsc",
    "build:prod": "npm run db:generate && tsc --project tsconfig.prod.json",
    "prestart": "npm run build",
    "start": "node dist/server.js",
    "start:prod": "NODE_ENV=production node dist/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:docker": "docker-compose -f docker-compose.test.yml up --build --abort-on-container-exit",
    "db:generate": "prisma generate",
    "db:generate:docker": "prisma generate --schema=./prisma/schema.prisma",
    "postinstall": "npm run db:generate",
    "docker:postinstall": "npm run db:generate:docker",
    "db:migrate": "prisma migrate dev",
    "db:migrate:prod": "prisma migrate deploy",
    "db:studio": "prisma studio",
    "db:seed": "ts-node prisma/seed.ts",
    "db:seed:prod": "node dist/prisma/seed.js",
    "db:backup": "./scripts/backup.sh",
    "db:restore": "./scripts/restore.sh",
    "docker:build": "docker build -t customer-success-backend .",
    "docker:run": "docker run -p 5000:5000 customer-success-backend",
    "docker:dev": "docker-compose up --build",
    "docker:prod": "docker-compose -f docker-compose.prod.yml up --build -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "health": "curl -f http://localhost:${PORT:-5000}/api/health || exit 1",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean"
  },
  "keywords": [
    "nodejs",
    "express",
    "prisma",
    "postgresql",
    "api"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^5.7.0",
    "@types/helmet": "^0.0.48",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-rate-limit": "^6.11.2",
    "express-validator": "^6.15.0",
    "helmet": "^6.2.0",
    "jsonwebtoken": "^9.0.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.0",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/morgan": "^1.9.4",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.15.11",
    "@types/supertest": "^2.0.12",
    "jest": "^29.5.0",
    "nodemon": "^3.1.10",
    "prisma": "^5.7.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2"
  }
}
